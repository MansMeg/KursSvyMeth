\begin_layout Itemize
Designvikten 
\series bold
vid OSU
\series default
 är 
\begin_inset Formula 
\[
w_{i}=\frac{1}{\pi_{i}}=\frac{N}{n}
\]

\end_inset


\end_layout

\begin_layout Pause

\end_layout

\begin_layout Itemize
Vikter vid estimation 
\begin_inset Formula $(*)$
\end_inset

 
\begin_inset Formula 
\[
\hat{t}_{\mathcal{U}}=\hat{t}_{HT}=\sum_{i\in\mathcal{S}}w_{i}y_{i}=\sum_{i\in\mathcal{S}}\frac{y_{i}}{\pi_{i}}
\]

\end_inset


\begin_inset Formula 
\[
\hat{\bar{y}}_{\mathcal{U}}=\frac{\hat{t}_{\mathcal{U}}}{N}=\frac{\sum_{i\in\mathcal{S}}w_{i}y_{i}}{\sum_{i\in\mathcal{S}}w_{i}}
\]

\end_inset

då
\begin_inset Formula 
\[
\sum_{i=1}^{N}\pi_{i}=n\mbox{ och }\sum_{i=1}^{n}w_{i}=N
\]

\end_inset


\end_layout

\begin_layout Itemize
Denna estimator kallas 
\series bold
Horwitz-Thompson
\series default
 estimatorn.
\end_layout

\begin_layout Pause

\end_layout

\begin_layout Itemize
Detta går enkelt att vidareutveckla för stratifierade urval 
\end_layout

\begin_layout EndFrame

\end_layout

\begin_layout BeginFrame
Urvalsvikter vid stratifiering 
\end_layout

\begin_layout Itemize
För ett stratifierat urval får vi samma formel för totalskattningen 
\begin_inset Formula $(*)$
\end_inset

:
\begin_inset Formula 
\[
\hat{t}_{\mathcal{U},str}=\hat{t}_{HT}=\sum_{j\in\mathcal{S}}w_{j}y_{j}
\]

\end_inset


\end_layout

\begin_layout Itemize
För att beräkna 
\begin_inset Formula $\hat{\bar{y}}_{\mathcal{U},str}$
\end_inset

 används de 
\begin_inset Quotes eld
\end_inset

stratifierade
\begin_inset Quotes erd
\end_inset

 vikterna.
\begin_inset Formula 
\[
\hat{\bar{y}}_{\mathcal{U}}=\frac{\hat{t}_{\mathcal{U}}}{N}=\frac{\sum_{j\in\mathcal{S}}w_{j}y_{hj}}{\sum_{j\in\mathcal{S}}w_{j}}
\]

\end_inset

 Det går att visa att denna estimator är samma som estimatorn på sida 
\begin_inset CommandInset ref
LatexCommand ref
reference "str_estimator"

\end_inset

 
\begin_inset Formula $(*)$
\end_inset

.
\end_layout

\begin_layout Pause

\end_layout

\begin_layout Itemize
Vid proportionell allokering har vi ett specialfall
\begin_inset Formula 
\[
w_{hj}=\frac{N_{h}}{n_{h}}=\frac{N}{n}
\]

\end_inset

vilket innebär att vi får samma vikter i alla strata.
\end_layout

\begin_layout Itemize
Då 
\begin_inset Formula $w_{j}$
\end_inset

 inte beror på strata kallas urvalet 
\begin_inset Quotes eld
\end_inset

självvägt
\begin_inset Quotes erd
\end_inset


\end_layout

