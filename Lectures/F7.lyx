\series bold
 kända populationstotalerna
\series default

\begin_inset Formula 
\[
\sum_{i\in\mathcal{S}}w_{i}g_{i}x_{i}=\frac{t_{x}}{\hat{t}_{x}}\sum_{i\in\mathcal{S}}w_{i}x_{i}=\frac{t_{x}}{\hat{t}_{x}}\hat{t}_{x}=t_{x}
\]

\end_inset


\end_layout

