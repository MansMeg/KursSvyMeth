\begin_layout BeginFrame
Populationsparametrar av intresse
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
t_{y}=t_{\mathcal{U},y}=\sum_{i\in\mathcal{U}}y_{i}=\sum_{i=1}^{N}y_{i}=\mbox{Populationstotalen för variabel \ensuremath{y}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\bar{y}_{\mathcal{U}}=\frac{1}{N}\sum_{i\in\mathcal{U}}y_{i}=\mbox{Populationsmedelvärde för variabel \ensuremath{y}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
S_{y}=\sqrt{\frac{1}{N-1}\sum_{i=1}^{N}\left(y_{i}-\bar{y}_{\mathcal{U}}\right)^{2}}=\mbox{Populationsstandardavvikelse}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
B=\frac{\bar{y}_{\mathcal{U}}}{\bar{x}_{\mathcal{U}}}=\frac{t_{y}}{t_{x}}=\mbox{Populationskvoten}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
R=\frac{\sum_{i\in\mathcal{U}}(x_{i}-\bar{x}_{\mathcal{U}})(y_{i}-\bar{y}_{\mathcal{U}})}{(N-1)S_{y}S_{x}}=\mbox{Populationskorrelationskoefficienten}
\]

\end_inset


\end_layout

